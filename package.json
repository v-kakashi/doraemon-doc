{
  "name": "doraemon-doc",
  "version": "2.0.9",
  "description": "一款文档生成器，能把 markdown 文件转化成 HTML 网站，方便开发者编写组件的说明文档。",
  "main": "./bin/doraemon-doc.js",
  "directories": {
    "example": "examples"
  },
  "bin": {
    "doraemon-doc": "./bin/doraemon-doc.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && babel src --out-dir lib",
    "dev": "babel src -s --out-dir lib --watch",
    "doc": "npm run build && node ./bin/doraemon-doc.js doc --duoshuoName doraemon-doc",
    "dist": "npm run build && node ./bin/doraemon-doc.js doc --build --duoshuoName doraemon-doc --publicPath /__site",
    "lint": "eslint src",
    "eslint-fix": "eslint --fix src",
    "gh-pages": "npm run dist && gh-pages -d __site",
    "pub": "npm run build && npm publish && npm run gh-pages",
    "beta": "npm run build && npm publish --tag beta && npm run gh-pages",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-kakashi/doraemon-doc.git"
  },
  "author": "lmm0591@qq.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/v-kakashi/doraemon-doc/issues"
  },
  "homepage": "https://github.com/v-kakashi/doraemon-doc#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "commander": "^2.9.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "dora": "^0.4.3",
    "doraemon-md-loader": "^2.1.3",
    "ejs": "^2.5.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "koa-webpack-dev-middleware": "^1.4.5",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "vue": "^2.2.6",
    "vue-loader": "^11.3.4",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.4.1",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "eslint": "^2.0.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "pre-commit": "^1.2.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "pre-commit": [
    "lint"
  ]
}
